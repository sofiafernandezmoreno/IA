<category>
	<pattern>DESCARTAR *</pattern>
	<template>
		<think>
			<set var="encontrado">
				<srai>FINDITEM <star/> IN <get name="list_fruit"/></srai>
			</set>
			<condition var="encontrado">
				<li value="0"></li>
				<li><set name="list_fruit"><srai>REMOVEITEM <get var="encontrado"/> IN <get name="list_fruit"/></srai></set></li>
			</condition>
		</think>
	</template>
</category>

